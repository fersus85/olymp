{
  "version": 1,
  "disable_existing_loggers": false,
  "formatters": {
    "simple": {
      "format": "%(asctime)s %(levelname)s %(message)s %(module)s",
      "datefmt": "%Y-%m-%dT%H:%M:%S%z"
    }
  },
"handlers": {
  "stderr": {
    "class": "logging.StreamHandler",
    "level": "WARNING",
    "stream": "ext://sys.stderr",
    "formatter": "simple"
    },
  "file": {
    "class": "logging.handlers.RotatingFileHandler",
    "level": "DEBUG",
    "formatter": "simple",
    "filename": "script/logs/app.log",
    "maxBytes": 100000,
    "backupCount": 3
  }
  },
"loggers": {
  "root": {
      "level": "DEBUG",
      "handlers": [
        "stderr",
        "file"
      ]
    }
  }
}